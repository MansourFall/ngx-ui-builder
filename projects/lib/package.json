{
  "name": "@sinequa/ngx-ui-builder",
  "version": "1.0.0",
  "author": "Sinequa",
  "description": "A library for making Angular applications easily configurable by end-users, via point-and-click and drag-and-drop interactions",
  "keywords": ["no code", "drag and drop", "point and click", "ui builder", "configurable"],
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.schematics.json",
    "postbuild": "copyfiles schematics/*/schema.json schematics/*/files/** schematics/collection.json ../../dist/lib/"
  },
  "schematics": "./schematics/collection.json",
  "ng-add": {
    "save": "dependencies"
  },
  "peerDependencies": {
    "@angular/common": "^12.2.5",
    "@angular/core": "^12.2.5"
  },
  "dependencies": {
    "tslib": "^2.3.0",
    "@ngneat/elf": "^1.0.2",
    "@ngneat/elf-entities": "^3.0.0",
    "@ngneat/elf-state-history": "^1.0.1",
    "bootstrap": "^5.1.3",
    "ngx-drag-drop": "^2.0.0"
  },
  "devDependencies": {
    "copyfiles": "file:../../node_modules/copyfiles",
    "typescript": "file:../../node_modules/typescript"
  }
}